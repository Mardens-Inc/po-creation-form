{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "main"
  ],
  "permissions": [
    "window-state:default",
    "core:window:allow-set-decorations",
    "core:window:allow-show",
    "core:event:default",
    "core:webview:allow-webview-position",
    "dialog:allow-ask",
    "dialog:allow-confirm",
    "dialog:allow-message",
    "dialog:allow-open",
    "dialog:allow-save",
    "dialog:default"
  ]
}